        "required": [
          "home_team",
          "away_team"
        ]
      },
      "PredictionResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "prediction": {
            "type": "string"
          },
          "confidence": {
            "type": "number"
          },
          "factors": {
            "type": "object"
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "status_code": {
            "type": "integer"
          }
        }
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key"
      }
    }
  }
